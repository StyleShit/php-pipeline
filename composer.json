{
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.22"
    },
    "scripts": {
        "lint": "pint",
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "StyleShit\\Pipeline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleShit\\Pipeline\\Tests\\": "tests"
        }
    }
}
